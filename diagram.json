{
  "version": "1.0",
  "author": "Kelompok 5 - IoT 1",
  "created": "2024",
  "description": "Diagram koneksi untuk kontrol LED dengan push button",
  "connections": [
    {
      "component": "LED Merah",
      "pin": "9",
      "type": "PWM Output"
    },
    {
      "component": "LED Hijau",
      "pin": "10",
      "type": "PWM Output"
    },
    {
      "component": "LED Biru",
      "pin": "11",
      "type": "PWM Output"
    },
    {
      "component": "Push Button Mode",
      "pin": "2",
